<mvc:View
    controllerName="eggo.datepickertest.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
<Panel 
	id="datePanel"
	width="auto">
	<DatePicker
		id="DPI"
		displayFormat="short"
		class="sapUiSmallMarginBottom"/>
	<DatePicker
		id="DPF"
		displayFormat="short"
		change="handleChange"
		class="sapUiSmallMarginBottom"/>
	<DatePicker
		id="DP"
		displayFormat="short"
		class="sapUiSmallMarginBottom"/>
</Panel>
</mvc:View>
